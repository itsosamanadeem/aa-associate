<odoo>

    <record id="view_donor_sub_group_tree" model="ir.ui.view">
        <field name="name">donor.sub.group.list</field>
        <field name="model">donor.sub.group</field>
        <field name="arch" type="xml">
            <list editable="bottom" decoration-success="True" default_order="sequence">
                <field name="sequence" widget="handle" />
                <field name="name"/>
            </list>
        </field>
    </record>

    <record id="action_donor_sub_group" model="ir.actions.act_window">
        <field name="name">Donor Sub Category</field>
        <field name="res_model">donor.sub.group</field>
        <field name="view_mode">list</field>
    </record>

</odoo>

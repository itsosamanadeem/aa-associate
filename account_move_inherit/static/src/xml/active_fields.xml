<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="account_move_inherit.ActiveFields" owl="1">
    <div class="d-flex flex-row align-items-center gap-4">
        <Many2XAutocomplete
            resModel="'res.partner.trademark'"
            value="props.record.data.trademark_id && props.record.data.trademark_id[0]"
            update.bind="this.onUpdateTrademark"
            getDomain.bind="this.getDomain"
            fieldString="'Trademark'"
            activeActions="{}"
        />
        <CheckBox
            class="'form-check-input'"
            checked="props.record.data.active or false"
        />
    </div>
    </t>


</templates>

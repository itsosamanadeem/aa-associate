<?xml version="1.0" encoding="UTF-8"?>

<templates xml:space="preserve">

    <t t-name="account_move_inherit.ApplicationNumberField" >
        <div>
            <div>
                <select>
                    <t t-foreach="this.variant_names" t-as="variant_name" t-key="variant_name">
                        <option t-esc="variant_name"/>
                    </t>
                </select>
            </div>
            <div>
                <input type="number" class="form-control" style="width:100px !important;"/>
            </div>
        </div>
    </t>

</templates>

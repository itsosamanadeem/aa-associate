<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">

    <t t-name="account_move_inherit.ApplicationNumberField">
        <t t-foreach="state.variant_names" t-as="variant_name" t-key="variant_name">
            <div class="d-flex align-items-center gap-2">
                <div>
                    <em>
                        <t t-out="variant_name"/>
                    </em>
                </div>
                <div>
                    <Field
                        name="'dynamic_field_' + variant_name"
                        type="'integer'"
                        value="state.values[variant_name] || 0"
                        onChange="(val) => this.onValueChange(variant_name, val)"
                    />
                </div>
            </div>
        </t>
    </t>

</templates>

<odoo>
    <record id="view_crm_partner_form_inherit_trademark_history" model="ir.ui.view">
        <field name="name">crm.partner.form.inherit.trademark.history</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml">

            <xpath expr="//form/sheet/notebook" position="inside">
                <page string="Trademark History" name="trademark_history_page">
                    <field name="trademark_history_ids">
                        <tree >
                            <field name="name"/>
                            <!-- <field name="services_taken"/> -->
                            <!-- <field name="start_date"/> -->
                            <!-- <field name="end_date"/> -->
                            <!-- <field name="trademark_id"/> -->
                            <!-- <field name="total_fee"/> -->
                            <!-- <field name="fee_per_class"/> -->
                            <!-- <field name="currency_id"/> -->
                            <!-- <field name="status"/> -->
                        </tree>
                        <form>
                            <sheet>
                                <group>
                                    <group>
                                        <field name="name"/>
                                        <field name="services_taken"/>
                                        <field name="start_date"/>
                                        <field name="end_date"/>
                                        <field name="case_description"/>
                                        <field name="trademark_id"/>
                                        <field name="add_file" filename="add_file_filename" widget="file"/>
                                        <field name="add_file_filename" invisible="1"/>
                                    </group>
                                    <group>
                                        <field name="currency_id"/>
                                        <field name="fee_per_class"/>
                                        <field name="total_fee"/>
                                        <field name="status" widget="statusbar" options="{'clickable': true}"/>
                                    </group>
                                </group>
                            </sheet>
                        </form>
                    </field>
                </page>
            </xpath>

        </field>
    </record>
</odoo>
